<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".BaseApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeLearn"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeLearn">
        </activity>
        <activity
            android:name=".StateActivity"
            android:exported="true"
            android:theme="@style/Theme.ComposeLearn">

        </activity>

        <activity
            android:name=".viewpager.ViewPagerActivity"
            android:exported="true"
            android:theme="@style/Theme.ComposeLearn">

        </activity>

        <activity
            android:name=".navigation.NavigationActivity"
            android:exported="true"
            android:theme="@style/Theme.ComposeLearn">

        </activity>

        <activity
            android:name=".navigation.AnotherActivity"
            android:exported="true"
            android:theme="@style/Theme.ComposeLearn">

        </activity>

        <activity
            android:name=".voyagerNavigation.VoyagerActivity"
            android:exported="true"
            android:theme="@style/Theme.ComposeLearn">

        </activity>

        <activity
            android:name=".voyagerNavigation.BottomNavigationActivity"
            android:exported="true"
            android:theme="@style/Theme.ComposeLearn">

        </activity>

        <activity
            android:name=".jetpackBottomNavigationView.JetpackBottomNavigation"
            android:exported="true"
            android:theme="@style/Theme.ComposeLearn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>